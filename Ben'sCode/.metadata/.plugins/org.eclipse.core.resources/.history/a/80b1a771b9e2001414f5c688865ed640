package warmUp;

import java.util.ArrayList;

public class ArrayM {
private int[] List;
	public ArrayM(int[] List) {
		List = this.List;
	}
	public ArrayList<Integer> getEven() {
		ArrayList<Integer> K = new ArrayList<Integer>();
		for(int i = 0; i < List.length; i++) {
			K.add(List[i]);
		}
			for(int i = 0; i < K.size(); i++) {
				if((K.get(i) % 2) != 0) {
					K.remove(i);
					//i--;
				}
			}
		
		
		return K;
	}
}
