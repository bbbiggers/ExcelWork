package warmUp;

import java.util.ArrayList;

public class Interest {

	public static void main(String[] args) {
		ArrayList<Integer> K = new ArrayList<Integer>();
		horse(K);
	}
	
	public static void horse(ArrayList<Integer> K) {
		for(int i = 0; i < K.size(); i++) {
			if(K.get(i) < 0) {
				K.remove(i);
			}
			if((K.get(i) % 2) == 0) {
				int temp = K.get(i);
				K.remove(i);
				K.add(0, temp);
			}
		}
	}
}
