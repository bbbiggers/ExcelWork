	public static String AtoB(String Let) {
		if(Let.equals("0")) {
			return "A";
		}
		else if(Let.equals("1")) {
			return "B";
		}
		else if(Let.equals("2")) {
			return "C";
		}
		else if(Let.equals("3")) {
			return "D";
		}
		else if(Let.equals("4")) {
			return "E";
		}
		else if(Let.equals("5")) {
			return "F";
		}
		else if(Let.equals("6")) {
			return "G";
		}
		else {
			return "G";
		}
		
		
	}
}
