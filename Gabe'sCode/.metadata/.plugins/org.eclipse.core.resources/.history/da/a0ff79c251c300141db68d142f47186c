package Warmups;

public class fibbonacciNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int n = 6;
fibbon(n);
	}
public static void fibbon(int n){

int temp = 1;	
for(int i = 1;i<=n;i++){
if(i == 1){
	System.out.println(1);
	
		temp = 1;
	}else{
		System.out.println(i+temp);
		temp = i;
	}
}
}

	
}


