package Warmups;

public class recusion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
String fart = "cats";
int n = fart.length();
dog(fart,n);

	}
public static void dog(String cat,int n){
	//int n = cat.length();
	if(n==0){
		System.out.println("");
	}
	else{System.out.print(cat.substring(n-1,n));
	n--;
	dog(cat,n);
	}
}
}