package Warmups;

public class doubleMethods {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println(cats(4));
		System.out.println(dogs(4));
		
	}

	
	
	public static double cats(int x){
	double newX = (double)(x);
	return (newX/3.0);
	}
	
	public static String dogs(int x){
		double fuasdx = (double)(x/3);
		return ""+fuasdx;
	}
	
	
}
