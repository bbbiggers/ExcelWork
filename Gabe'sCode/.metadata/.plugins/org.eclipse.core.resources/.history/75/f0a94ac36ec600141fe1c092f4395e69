package Warmups;

public class findingDigit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a = 77771;
		System.out.println(frog(a));
		
		
	}

	public static int frog(int a){
		if(a<7){
			return 0;
		}
		if(a%10 == 7){
			a=a/10;
			return 1+frog(a);
		}
		else{
			a=a/10;
			return frog(a);
		}
	}
	
	
	
}
